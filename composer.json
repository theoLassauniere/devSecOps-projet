{
  "type": "project",
  "description": "Simple PHP project for the DevSecOps class",
  "require": {
    "php": ">=8.2.0",
    "ext-gd": "*",
    "nesbot/carbon": "^2.72.1",
    "vlucas/phpdotenv": "^5.6.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "phpunit/phpunit": "^9.6",
    "phpmetrics/phpmetrics": "^2.9.1",
    "phploc/phploc": "^7.0.2",
    "phpmd/phpmd": "^2.15",
    "niels-de-blaauw/php-doc-check": "^0.4.1",
    "squizlabs/php_codesniffer": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  }
}
